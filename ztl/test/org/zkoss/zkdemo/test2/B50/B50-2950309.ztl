<test tags="B50-2950309.zul,A,E,Datebox">
	<case id="closed">
		<server>
			<!--
			<zk>
				<datebox id="db" readonly="true" />
				<button id="btn" label="disabled" onClick="db.disabled=!db.disabled" />
			</zk>	
			-->
		</server>
		<client wait="100"><![CDATA[
				click(db.$n("btn"));
				waitResponse();
				verifyTrue(isVisible(jq(".z-datebox-pp")));
				click(db.$n("btn"));
				click(btn);
				waitResponse();
				click(db.$n("btn"));
				waitResponse();
				verifyFalse(isVisible(jq(".z-datebox-pp")));
				click(db.$n("btn"));
				click(btn);
				waitResponse();
				click(db.$n("btn"));
				waitResponse();
				verifyTrue(isVisible(jq(".z-datebox-pp")));
			]]></client>
	</case>
</test>