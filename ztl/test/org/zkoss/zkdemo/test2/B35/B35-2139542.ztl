<test tags="B35-2139542.zul,A,E,Datebox,Calendar,ie9">
	<case id="ztl">
		<server><!--
			


<hbox>
	<datebox id="db" />
	<button label="Click Me without error, that is correct!" onClick='db.setFormat("dd-MMM-yyyy")'/>
</hbox>

		--></server>
		<client><![CDATA[
			click(jq("@button"));
			verifyFalse(jq(".z-errbox").exists());
			
			Element $db = db.$n("real");
			focus($db);
			type($db,"asd");
			blur($db);
			verifyTrue(jq(".z-errbox").exists());
	
		]]></client>
	</case>
</test>