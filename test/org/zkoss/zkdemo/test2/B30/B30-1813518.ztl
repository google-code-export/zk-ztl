<test tags="Xhtml,Component,Directive"
	action="/test2/B30-1813518.zhtml">
	<case id="Style">
		<client><![CDATA[
			if (ZK.is("ie")) {
				assertEquals("blue 1px solid", jq("@rows:eq(0) td").css("border"));
				assertEquals("#333333", jq("@rows:eq(0) td").css("color"));
				assertEquals("#e3ebf6", jq("@rows:eq(0) td").css("backgroundColor"));

				assertEquals("null", jq("@rows:eq(1) td").css("border"));
				assertEquals("#000000", jq("@rows:eq(1) td").css("color"));
				assertEquals("white", jq("@rows:eq(1) td").css("backgroundColor"));
			} else {
				assertEquals("1px solid blue", jq("@rows:eq(0) td").css("border"));
				assertEquals("rgb(51, 51, 51)", jq("@rows:eq(0) td").css("color"));
				assertEquals("rgb(227, 235, 246)", jq("@rows:eq(0) td").css("backgroundColor"));

				assertEquals("", jq("@rows:eq(1) td").css("border"));
				assertEquals("rgb(0, 0, 0)", jq("@rows:eq(1) td").css("color"));
				assertEquals("rgb(255, 255, 255)", jq("@rows:eq(1) td").css("backgroundColor"));
			}
		]]></client>
	</case>
</test>