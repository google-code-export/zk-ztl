<test tags="menupopup">
	<case id="RightClick">
		<server><!--
			<window id="win" context="menu" border="normal" contentStyle="background-color:blue">
				<menupopup id="menu">
					<menu label="Save"/>
				</menupopup> 
			</window>
		--></server>
		<client>
			contextMenu(win);
			assertTrue(isVisible(menu));
		</client>
	</case>
</test>