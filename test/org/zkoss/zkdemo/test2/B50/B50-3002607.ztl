<test tags="textbox">
	<case id="deleteChar">
		<server id="zul"><![CDATA[
			<zk>
				<bandbox id="bb" readonly="true">
					<bandpopup>
						<textbox id="tb" value="test" />
					</bandpopup>
				</bandbox>
			</zk>
		]]></server>
		<client><![CDATA[
				click(bb.$n("btn"));
				waitResponse();
				String before = jq(".z-textbox").val();
				focus(tb.$n());
				keyPressNative(END);
				waitResponse();
				keyPressNative(BACKSPACE);
				waitResponse();
				verifyFalse(jq(".z-textbox").val().equals(before));
		]]></client>
	</case>
</test>