<test tags="B50-3259479.zul,A,E,Zscript">
	<case id="ztl">
		<server><!--
			<zk>
	<html><![CDATA[
	<ul>
		<li>You shall see HelloZK below.</li>
	</ul>
	]]></html>
	<zscript>
	List list = new ArrayList();
	list.add("Hello");
	list.add("ZK");
	</zscript>
	<div>
		<label forEach="${list}">
			<zscript>
			self.setValue(each);
			</zscript>
		</label>
	</div>
</zk>

		--></server>
		<client><![CDATA[
				verifyEquals("HelloZK",jq("@label").text());
		]]></client>
	</case>
</test>