<test tags="combobox,popup">
	<case id="zindex">
		<server id="zul"><![CDATA[
			<zk>
				<combobox id="cb">
					<comboitem tooltip="popup" label="test" id="cbi"/>
				</combobox>
				<popup id="popup">
					<label value="popup"/>
				</popup>
			</zk>
		]]></server>
		<client><![CDATA[
			click(cb.$n("btn"));
			mouseOver(cbi);
			String zindex = jq(cb.$n("pp")).css("z-index");
			int ppZindex = zindex.equals("auto") ? 0 : Integer.parseInt(zindex);
			zindex = jq(popup).css("z-index");
			int popipZindex = zindex.equals("auto") ? 0 : Integer.parseInt(zindex);
			assertTrue("The z-index of the popup: " +popipZindex + " isn't greate then the pp of the combobox: "+ppZindex,popipZindex > ppZindex);
		]]></client>
	</case>
</test>