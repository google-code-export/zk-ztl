<test tags="tabbox,popup">
	<case id="Popup">
		<server><!--
		<zk>
			<tabbox>
				<tabs>
					<tab label="tab1"/>
				</tabs>
				<tabpanels>
					<tabpanel visible="false">
						<button id="btn" label="click me" popup="pp"/>
						<popup id="pp" width="300px">
							Here is a Popup.
						</popup>
					</tabpanel>
				</tabpanels>
			</tabbox>
		</zk>
		--></server>
		<client>
			click(btn);
			assertTrue(isVisible(pp));
		</client>
	</case>
</test>