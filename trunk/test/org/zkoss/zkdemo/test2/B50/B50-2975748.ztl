<test tags="button,popup">
	<case id="popup">
		<server id="zul"><![CDATA[
			<zk>
			   <button id="btn" popup="popup" onClick="" />
			   <popup id="popup"/>
			</zk>
		]]></server>
		<client wait="100"><![CDATA[
			click(btn);
			assertTrue(popup.exists());
		]]></client>
	</case>
</test>