<test tags="button,label">
	<case id="event">
		<server><!--
			<zk>
				After clicking the test button, you shall see "&lt;Button b>:[MouseEvent onClick &lt;Button b>]:{}:void" below
				<separator/>
				<label id="i"/>
				<zscript>
					public void test(){i.value = self+":"+event+":"+componentScope+":"+b;}
				</zscript>
				<window>
				<button id="b" label="test" onClick="test()"/>
				</window>
			</zk>
		--></server>
		<client><![CDATA[
			click(b);
			assertEquals("<Button b>:[MouseEvent onClick <Button b>]:{}:void",i.get("value"));
		]]></client>
	</case>
</test>