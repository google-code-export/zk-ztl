<test tags="intbox">
	<case id="paste">
		<server><!--
			<zk>
				now support copy paste by ctrl key in intbox
				<intbox value="1234567"/>
			</zk>
		--></server>
		<client><![CDATA[
			focus(jq("@intbox"));
			controlKeyDown();
			keyPress(jq("@intbox"), A);
			keyPress(jq("@intbox"), X);
			verifyEquals("", getValue(jq("@intbox")));
			keyPress(jq("@intbox"), V);
			controlKeyUp();
			verifyEquals("1234567", getValue(jq("@intbox")));
		]]></client>
	</case>
</test>