<test tags="grid,forEach">
	<case id="ForEach">
		<server><!--
<zk>
	expect to get 100 ~ 104 in first row, then 105 ~ 109 for next row.
	<zscript>
	int[] counts = new int[2];
	String[] labels = new String[] {"100", "101", "102", "103", "104", "105",
	"106", "107", "108", "109", "110"};
	</zscript>
	<grid>
	<rows>
	
	<row forEach="${counts}">
		<button forEach="${labels}"
		forEachBegin="${forEachStatus.previous.index * 5}"
		forEachEnd="${forEachStatus.previous.index * 5 + 4}"
		label='${each} , ${forEachStatus.previous.index}'/>
		</row>
	</rows>
	</grid>
</zk> 
		--></server>
		<client><![CDATA[
		assertEquals("100 , 0", widget(jq("@button:eq(0)")).get("label"));
		assertEquals("101 , 0", widget(jq("@button:eq(1)")).get("label"));
		assertEquals("102 , 0", widget(jq("@button:eq(2)")).get("label"));
		assertEquals("103 , 0", widget(jq("@button:eq(3)")).get("label"));
		assertEquals("104 , 0", widget(jq("@button:eq(4)")).get("label"));
		assertEquals("105 , 1", widget(jq("@button:eq(5)")).get("label"));
		assertEquals("106 , 1", widget(jq("@button:eq(6)")).get("label"));
		assertEquals("107 , 1", widget(jq("@button:eq(7)")).get("label"));
		assertEquals("108 , 1", widget(jq("@button:eq(8)")).get("label"));
		assertEquals("109 , 1", widget(jq("@button:eq(9)")).get("label"));
		]]></client>
	</case>
</test>